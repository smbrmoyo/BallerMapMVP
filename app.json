{
  "expo": {
    "name": "BallerMapMVP",
    "slug": "BallerMapMVP",
    "description": "An app that uses your location to find basketball courts near users",
    "version": "1.0.1",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "userInterfaceStyle": "automatic",
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "buildNumber": "3",
      "supportsTablet": false,
      "userInterfaceStyle": "automatic",
      "usesAppleSignIn": false,
      "config": {
        "googleMapsApiKey": "AIzaSyCAWRoRAT1jDaCuwACpmYsseOgW1-_XrNg"
      },
      "infoPlist": {
        "UIBackgroundModes": ["location", "fetch"],
        "NSLocationAlwaysAndWhenInUseUsageDescription": "BallerMap requires geolocation to improve the quality of the service"
      },
      "bundleIdentifier": "com.smbrmoyo.BallerMap"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#FFFFFF"
      }
    },
    "web": {
      "favicon": "./assets/favicon.png"
    }
  }
}
